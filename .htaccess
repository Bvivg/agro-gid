<IfModule mod_rewrite.c>
  RewriteEngine on
  RewriteBase /

  # Rewrite for search query
  RewriteRule ^workers/([^/]+)/?$ /workers?searchWorkers=$1 [QSA,L]

  # Your existing rules
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule (.*) /index.php?$1 [QSA,L]
</IfModule>